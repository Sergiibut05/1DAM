Proceso sumarN1N2
	escribir "Escribe el primer número del intervalo: "Sin Saltar;
	Definir n1 Como Entero;
	leer n1;
	Escribir "Escribe el segundo número del intervalo: "Sin Saltar;
	definir n2 como entero;
	leer n2;
	Definir suma Como Entero;
	definir i como entero;
	suma <- 0;
	Para i<-n1 Hasta n2 Con Paso 1 Hacer
		suma <- suma + i;
	FinPara
	Escribir suma;
	
FinProceso
